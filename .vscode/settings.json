{
  "editor.wordBasedSuggestions": "off",
  "markdownlint.config": {
    "MD013": false,
    "MD033": false
  },
  "latex-workshop.chktex.path": "",
  "latex-workshop.latex.additionalBib": [
    "/Users/hiromi/Library/texmf/bibtex/bib/myreference.bib"
  ],
  "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
  "latex-workshop.latex.recipes": [],
  "insertDateString.format": "YYYY/MM/DD hh:mm:ss JST",
  "insertDateString.formatDate": "YYYY/MM/DD",
  "haskell.formattingProvider": "fourmolu",
  "emeraldwalk.runonsave": {
    "commands": [
      { "cmd": "hpack", "isAsync": true, "match": "(package\\.yaml|\\.l?hs)$" }
    ]
  },
  "path-intellisense.absolutePathTo": "${workspaceFolder}/_site",
  "path-intellisense.absolutePathToWorkspace": true,
  "path-autocomplete.pathMappings": {
    "${root}": {
      "conditions": [
        {
          "when": "site-src",
          "value": "${folder}/_site"
        },
        {
          "when": "src",
          "value": "${folder}"
        }
      ]
    }
  },
  "path-autocomplete.ignoredFilesPattern": ".DS_Store"
}
